
    <input type='hidden' name='_method' value='put'>

    <div class="page-header page-header-bordered page-header-tabs">

    <figure class='logo-projeto'>
                <div class='img-responsive  center-block' style='width:300px;'>
                  <input type="file" id="input-file-now-custom-1"
                  data-default-file='<%= @project.image_url if @project %>' data-plugin="dropify"/>
              </div>
    </figure>
    <h1  class="page-title" data-plugin="summernote" data-air-mode="true"><%= @project.name %></h1>
    <p class='description' data-plugin="summernote" data-air-mode="true"><%= @project.description %></p>

      <ul class="nav nav-tabs nav-tabs-line" data-plugin="nav-tabs" role="tablist" >
        <li class="active">
          <a data-toggle="tab" href="#about_aba"><%=t('About')%></a>
        </li>
        <li>
          <a data-toggle="tab"  href="#how-to-start"><%=t('How to start')%></a>
        </li>
        <li>
          <a data-toggle="tab" href="#config-workspace"><%=t('Configure workspace')%></a>
        </li>
        <li>
          <a data-toggle="tab" href="#know-code"><%=t('Know the code')%></a>
        </li>
        <li>
          <a data-toggle="tab" href="#send_contribution"><%=t('Send contribution')%></a>
        </li>
        <li>
          <a data-toggle="tab" href="#owners"><%= t('Owners')%></a>
        </li>
       </ul>

    </div>
    <div class="page-content container-fluid tab-content">
    <div class="grid-stack tab-pane animation-fade active" data-plugin="gridstack" data-gs-width="12"  id="about_aba" data-gs-animate="yes" role="tabpanel" >
    <!-- TAB ABOUT -->
    <% @project.widgets.from_tab_about.each do |widget| %>
      <%= render 'widget', :widget => widget %>
    <% end %>
    </div>
    <!-- TAB HOW TO START-->
    <div class="grid-stack  tab-pane animation-fade" data-plugin="gridstack" data-gs-width="12" id="how-to-start"
      data-gs-animate="yes" role="tabpanel" data-gs-no-resize="" data-gs-no-move="" data-gs-locked="" >
      <% @project.widgets.from_tab_how_to_start.each do |widget| %>
        <%= render 'widget', :widget => widget %>
      <% end %>
    </div>
    <!--TAB CONFIGURE WORKSPACE-->
    <div class="grid-stack  tab-pane animation-fade" data-plugin="gridstack" data-gs-width="12" id="config-workspace"
      data-gs-animate="yes" role="tabpanel" data-gs-no-resize="" data-gs-no-move="" data-gs-locked="" >

      <% @project.widgets.from_tab_configure_workspace.each do |widget| %>
        <%= render 'widget', :widget => widget %>
      <% end %>
    </div>
      <!-- TAB KNOW THE CODE -->
      <div class="grid-stack  tab-pane animation-fade" data-plugin="gridstack" data-gs-width="12" id="know-code"
      data-gs-animate="yes" role="tabpanel" data-gs-no-resize="" data-gs-no-move="" data-gs-locked="">
    <% @project.widgets.from_tab_know_the_code.each do |widget| %>
      <%= render 'widget', :widget => widget %>
    <% end %>
    </div>
      <!-- TAB SEND CONTRIBUTION -->
    <div class="grid-stack  tab-pane animation-fade" data-plugin="gridstack"  data-gs-width="12" id="send_contribution" data-gs-animate="yes"  role="tabpanel" data-gs-no-resize="" data-gs-no-move="" data-gs-locked="" >
        <% @project.widgets.from_tab_send_contribution.each do |widget| %>
          <%= render 'widget', :widget => widget %>
        <% end %>
      </div>

      <div class="grid-stack  tab-pane animation-fade" data-plugin="gridstack" data-gs-width="12" id="owners" data-gs-animate="yes" role="tabpanel" data-gs-no-resize="" data-gs-no-move="" data-gs-locked="">
    <%= render 'widget_owner' %>

      </div>


      </div>
      <!-- End Panel -->
